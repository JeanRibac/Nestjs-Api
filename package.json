{
  "name": "nestjs-task-management-mongodb-integration",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "concurrently": "^5.2.0"
  },
  "devDependencies": {},
  "scripts": {
    "prod": "cd client && npm run build && cd .. && cd server && npm run start",
    "dev": "concurrently \"npm run start:dev --prefix server\" \"npm run start --prefix client\"",
    "client": "cd client && npm install --save && npm run build",
    "server": "cd server && npm install",
    "inst": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run inst && node server/dist/main"
  },
